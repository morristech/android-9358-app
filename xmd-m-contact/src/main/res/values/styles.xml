<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ContactTypeTextStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/selector_contact_text_table</item>
    </style>

    <style name="search_contact_popup_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textColor">@color/alert_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/fontTitle</item>
    </style>
</resources>