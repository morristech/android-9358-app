<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--<item name="android:paddingTop">8dp</item>-->
    <!--<item name="android:paddingBottom">8dp</item>-->
    <style name="native_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">@dimen/mine_width</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">14sp</item>

    </style>

    <style name="horizontal_divider_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#e0e0e0</item>
    </style>

    <style name="vertical_divider_line">
        <item name="android:layout_width">0.5dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#e0e0e0</item>
    </style>

    <style name="native_employee_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">@dimen/mine_width</item>
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="dotted_line">
       <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/dotted_div_line</item>
        <item name="android:layerType">software</item>
    </style>
</resources>