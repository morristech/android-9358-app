<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_margin="12dp"
              android:background="@drawable/shape_bg_white_with_stoke"
              android:orientation="horizontal"
              android:padding="0.5dp">

    <cn.zhouchaoyuan.excelpanel.ExcelPanel
        android:id="@+id/service_excel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingRight="0.5dp"
        android:paddingTop="0.5dp"
        app:left_cell_width="@dimen/excel_row_width"
        app:normal_cell_width="@dimen/excel_cell_width"
        app:top_cell_height="@dimen/excel_cell_height"
    />
</LinearLayout>