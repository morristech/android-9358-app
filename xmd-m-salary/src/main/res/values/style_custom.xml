<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="salary_table_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/selector_tab_text</item>
        <item name="android:padding">4dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="reserve_data_table_line_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:colorBackground">@color/selector_salary_table_line</item>
    </style>

    <style name="salary_introduce_title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">#e9e0cf</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="salary_commission_title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:textColor">#333333</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="salary_commission_menu_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">13sp</item>
        <item name="android:background">#CAE0F1</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="salary_commission_package_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#333333</item>
        <item name="android:textSize">13sp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
    </style>

    <style name="horizontal_divider_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#e0e0e0</item>
    </style>

    <style name="vertical_divider_line">
        <item name="android:layout_width">0.5dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#e0e0e0</item>
    </style>
</resources>