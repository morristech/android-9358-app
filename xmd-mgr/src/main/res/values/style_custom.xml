    <style name="list_item_vertical_line" parent="vertical_line">
        <item name="android:background">@color/list_item_line_color</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:layout_marginRight">1dp</item>
    </style>

    <style name="line">
        <item name="android:background">@color/divider_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
    </style>

    <style name="line_vertical">
        <item name="android:background">#E0E0E0</item>
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_marginTop">@dimen/small_space</item>
        <item name="android:layout_marginBottom">@dimen/small_space</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="horizontal_line">
        <item name="android:background">@color/divider_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="line_margin">
        <item name="android:background">@color/divider_color</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginRight">@dimen/customer_comment_margin_right</item>
        <item name="android:layout_marginLeft">@dimen/customer_comment_margin_right</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="comment_line">
        <item name="android:background">@color/colorCommentDivide</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="border_line" parent="line">
        <item name="android:background">@color/border_line_color</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
    </style>

    <style name="list_item_line" parent="line">
        <item name="android:background">@color/list_item_line_color</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>

    <style name="dialog_line" parent="line">
        <item name="android:background">@color/colorPrimary</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
    </style>

    <style name="dialog_gray_line" parent="line">
        <item name="android:background">@color/dialog_line_gray</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_marginBottom">0dp</item>
