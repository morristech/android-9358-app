<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="wh_wrap_content_layout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="wh_match_parent_wrap_content_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="PopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_left_in</item>
        <item name="android:windowExitAnimation">@anim/slide_left_out</item>
    </style>

    <style name="anim_bottom_to_top_style">
        <item name="android:windowEnterAnimation">@anim/slide_bottom_top_alpha_in</item>
        <item name="android:windowExitAnimation">@anim/slide_top_bottom_alpha_out</item>
    </style>

    <style name="anim_top_to_bottom_style">
        <item name="android:windowEnterAnimation">@anim/slide_top_bottom_alpha_in</item>
        <item name="android:windowExitAnimation">@anim/slide_bottom_top_alpha_out</item>
    </style>

    <style name="anim_style_scale_out">
        <item name="android:windowEnterAnimation">@anim/scale_alpha_in</item>
        <item name="android:windowExitAnimation">@anim/scale_alpha_out</item>
    </style>

    <!-- Primary Button Style -->
    <style name="primary_button_style" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/selector_primary_button_bg</item>
        <item name="android:textColor">@color/primary_button_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0dp</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:textSize">@dimen/fontButton</item>
    </style>

    <!-- listitem confirm button -->

    <style name="operation_button_section_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="operation_button_style" parent="Widget.AppCompat.Button">
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/zero</item>
        <item name="android:textSize">@dimen/operation_button_text_size</item>
        <item name="android:width">60dp</item>
    </style>

    <style name="operation_negative_button" parent="operation_button_style">
        <item name="android:layout_width">@dimen/operation_button_width</item>
        <item name="android:layout_height">@dimen/operation_button_height</item>
        <item name="android:background">@drawable/refuse_order_btn_bg</item>
        <item name="android:textColor">@color/selector_operation_negative_button_text</item>
    </style>

    <style name="operation_positive_button" parent="operation_button_style">
        <item name="android:layout_marginLeft">@dimen/inter_space</item>
        <item name="android:layout_marginRight">@dimen/inter_space</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">@dimen/operation_button_width</item>
        <item name="android:layout_height">@dimen/operation_button_height</item>
        <item name="android:background">@drawable/selector_operation_positive_button_bg</item>
        <item name="android:textColor">@color/selector_operation_positive_button_text</item>
    </style>

    <!-- Custom Alert Dialog -->
    <style name="custom_alert_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="dialog_button_style" parent="Widget.AppCompat.Button">
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/zero</item>
        <item name="android:textSize">@dimen/dialog_button_text_size</item>
    </style>

    <style name="dialog_negative_button" parent="dialog_button_style">
        <item name="android:background">@drawable/selector_dialog_negative_button_bg</item>
        <item name="android:textColor">@color/colorClubItemTitle</item>
    </style>

    <style name="dialog_positive_button" parent="dialog_button_style">
        <item name="android:background">@drawable/selector_dialog_positive_button_bg</item>
        <item name="android:textColor">@color/colorClubItemTitle</item>
    </style>

    <style name="popup_window_button_style" parent="Widget.AppCompat.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">@dimen/popup_window_button_min_width</item>
        <item name="android:background">@drawable/selector_popup_window_button_bg</item>
        <item name="android:textColor">@color/selector_popup_window_button_text</item>
    </style>

    <style name="tab_vertical_line">
        <item name="android:background">@color/divider_color</item>
        <item name="android:layout_width">@dimen/line_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginBottom">@dimen/inter_space</item>
        <item name="android:layout_marginTop">@dimen/inter_space</item>
    </style>

    <style name="vertical_line">
        <item name="android:background">@color/divider_color</item>
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="publicity_vertical_line">
