<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xmd.technician">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" /> -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:name=".TechApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="DEF_UPDATE_SERVER"
            android:value="${DEF_UPDATE_SERVER}" />
        <meta-data
            android:name="PUSH_MASTERSECRET"
            android:value="${GETUI_MASTER_SECRET}" />

        <!-- umeng sdk -->
        <meta-data
            android:name="UMENG_APPKEY"
            android:value="${UMENG_APPKEY_VALUE}" />
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="${UMENG_CHANNEL_VALUE}" />
        <!-- end umeng sdk -->

        <!-- 设置环信应用的appkey -->
        <meta-data
            android:name="EASEMOB_APPKEY"
            android:value="${EASEMOB_APPKEY_VALUE}" />
        <!-- 声明sdk所需的service SDK核心功能 -->
        <service
            android:name="com.hyphenate.chat.EMChatService"
            android:exported="true" />
        <!-- 声明sdk所需的receiver -->
        <receiver android:name="com.hyphenate.chat.EMMonitorReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />
                <data android:scheme="package" />
            </intent-filter>
            <!-- 可选filter -->
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>

        <activity android:name=".window.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".window.LoginActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".window.MainActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:launchMode="singleTask"
            android:windowSoftInputMode="adjustPan" />

        <receiver
            android:name=".push.GetuiReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.igexin.sdk.action.${GETUI_APP_ID}" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".window.TechInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ModifyPasswordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ChatActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="com.xmd.technician.action.START_CHAT" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".window.BrowserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.CommentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.SettingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.FeedbackActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.AppInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.WorkTimeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.MyAccountActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ConsumeDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ServiceItemActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ClipPictureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.RegisterActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ResetPasswordActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.InfoInputActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.UploadAvatarActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.OrderDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ShowBigImageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.PaidCouponDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.PaidCouponUserDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.NormalCouponDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".wxapi.WXEntryActivity"
            android:exported="true"
            android:label="@string/wx_entry_activity_title"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.AddFriendActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.CellphoneContactListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.ContactInformationDetailActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".window.SysNoticeListActivity"></activity>

        <activity
            android:name=".window.EditContactInformation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.TechShareCardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.PreLoginActivity"
            android:launchMode="singleTask" />

        <activity
            android:name=".window.ConfigurationMonitorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.AvailableCouponListActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".window.UserCreditCenterActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity android:name=".window.CreditExchangeActivity">
            android:screenOrientation="portrait"
        </activity>
        <activity
            android:name=".window.CreditRuleExplainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.IntroduceAccountActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.DynamicDetailActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".window.OrderFragmentActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity android:name=".window.DynamicShareTechActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
    </application>

</manifest>